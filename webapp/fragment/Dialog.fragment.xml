<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <TableSelectDialog
        id="tableSelectDialog"
        title="User Name List"
        class="sapUiPopupWithPadding"
        items="{oUserList>/results}"
        search="_handleValueHelpSearch"
        confirm="_handleValueHelpClose"
        cancel="_handleValueHelpClose">
        <ColumnListItem id="_IDGenColumnListItem1">
            <cells>
                <Text id="_IDGenText1" text="{oUserList>Bname}" />
            </cells>
        </ColumnListItem>
        <columns>
            <Column id="_IDGenColumn1"
                hAlign="Begin"
                popinDisplay="Inline"
                demandPopin="true">
                <Label id="_IDGenLabel1" text="User Name" />
            </Column>
        </columns>
    </TableSelectDialog>
</core:FragmentDefinition>